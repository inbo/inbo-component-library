{
  "name": "@inbo/ng-inbo",
  "version": "0.0.55",
  "repository": {
    "type": "git",
    "url": "https://github.com/inbo/inbo-component-library.git"
  },
  "overrides": {
    "ip": "2.0.1"
  },
  "standard-version": {
    "skip": {
      "bump": true,
      "commit": true,
      "tag": true
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "tsc -p projects/ng-inbo/tsconfig.schematics.json",
    "lint": "ng lint",
    "build-schematics": "cd projects/ng-inbo && npm run build",
    "release": "cd projects/ng-inbo && standard-version && cd ../.. && git push --follow-tags && npm run build && cd dist/ng-inbo && npm publish",
    "release:major": "cd projects/ng-inbo && standard-version --release-as major && cd ../.. && git push --follow-tags && npm run build && cd dist/ng-inbo && npm publish",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "19.1.6",
    "@angular/cdk": "19.1.4",
    "@angular/common": "19.1.6",
    "@angular/compiler": "19.1.6",
    "@angular/core": "19.1.6",
    "@angular/forms": "19.1.6",
    "@angular/material": "19.1.4",
    "@angular/platform-browser": "19.1.6",
    "@angular/platform-browser-dynamic": "19.1.6",
    "@angular/router": "19.1.6",
    "@inbo/ng-inbo": "file:dist/ng-inbo",
    "@types/lodash-es": "4.17.12",
    "@types/proj4": "2.5.5",
    "copyfiles": "2.4.1",
    "date-fns": "2.29.3",
    "lodash-es": "4.17.21",
    "ngx-pipes": "3.2.2",
    "ol": "9.1.0",
    "proj4": "2.11.0",
    "rxjs": "7.8.1",
    "tslib": "2.4.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.1.7",
    "@angular/cli": "19.1.7",
    "@angular/compiler-cli": "19.1.6",
    "@johanblumenberg/ts-mockito": "1.0.43",
    "@types/jasmine": "4.0.0",
    "core-js": "3.37.0",
    "jasmine-core": "4.2.0",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ng-packagr": "19.1.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.5.4"
  }
}
